RewriteEngine on
RewriteRule ^cliente$ cliente.php
RewriteRule ^cliente/(.+)/(.+)/(.+)/(.+)/(.+)/(.+)/(.+)/(.+)/(.+)$ cliente.php?dniCliente=$1&$2=$3&$4=$5&$6=$7&$8=$9
RewriteRule ^cliente/(.+)/(.+)/(.+)/(.+)/(.+)/(.+)/(.+)$ cliente.php?dniCliente=$1&$2=$3&$4=$5&$6=$7
RewriteRule ^cliente/(.+)/(.+)/(.+)/(.+)/(.+)$ cliente.php?dniCliente=$1&$2=$3&$4=$5
RewriteRule ^cliente/(.+)/(.+)/(.+)$ cliente.php?dniCliente=$1&$2=$3
RewriteRule ^cliente/(.+)$ cliente.php?dniCliente=$1

